        "technodelight/php-cli-cross-platform-open": "^1.0",
        "technodelight/php-cli-editor-input": "^1.0",
        "technodelight/php-cli-iterm-image-renderer": "^1.0",
        "technodelight/php-jira-tempo-plugin-api": "^1.1",
        "technodelight/php-jira-domain-models": "^1.2",
        "technodelight/php-jira-cli-interfaces": "^1.0"
    },
    "require-dev": {
        "behat/behat": "^3.3",
        "phpspec/phpspec": "^2.5"
    },
    "authors": [
        {
            "name": "Zsolt GÃ¡l",
            "email": "zenc@zenc.hu"
        }
    ],
    "autoload": {"psr-0": {"Technodelight\\": "src"}},
    "config": {
      "bin-dir": "bin"
    },
    "scripts": {
        "test": [
            "@test-unit",
            "@test-acceptance"
        ],
        "test-unit": [
            "bin/phpspec --ansi run -fpretty"
        ],
        "test-acceptance": [
            "bin/behat --colors"
